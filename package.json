    "jquery.loadtemplate": "^1.5.10",
    "mongodb": "^3.1.0-beta4",
    "morgan": "^1.9.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "istanbul": "^0.4.5",
    "mocha": "^4.1.0",
    "should": "^13.2.1",
